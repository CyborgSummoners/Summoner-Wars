procedure list_demo is
	assignment_demo;
end list_demo;

procedure assignment_demo is
	print("Assignment demo:");
	i := "hello world";
	l := ["kutya", 1, 2, i, true, "teve"]; -- making a list
	b := l;
	l[0] := "krokodil"; -- element assignment
	print(l);
	print(b); -- observe assignment by reference;
end assignment_demo;
